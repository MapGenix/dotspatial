  <WizardData>
    <packages repository="extension"
              repositoryId="DotSpatial.Plugins.e9e9a389-0b83-44df-9681-ac1de97c386c">
      <package id="DotSpatial.Controls" version="1.0.0.0" />
      <package id="DotSpatial.Data" version="1.0.0.0" />
