<?xml version="1.0" encoding="utf-8"?>
<VSTemplate Version="3.0.0" Type="Project" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005">
  <TemplateData>
    <Name>DotSpatial.Plugins.ProjectTemplate</Name>
    <Description>A project for creating a Dotspatial extension which DotSpatial and Mapwindow 6.</Description>
    <Icon>DotSpatialGlobe.ico</Icon>
    <ProjectType>CSharp</ProjectType>
    <RequiredFrameworkVersion>4.0</RequiredFrameworkVersion>
    <SortOrder>1000</SortOrder>
    <TemplateID>55b906c4-f79a-4b19-a17c-7cd862938131</TemplateID>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>DotSpatial.Plugins.ProjectTemplate</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
  </TemplateData>
  <TemplateContent>
    <Project File="ProjectTemplate.csproj" ReplaceParameters="true">
      <ProjectItem ReplaceParameters="true" TargetFileName="Properties\AssemblyInfo.cs">AssemblyInfo.cs</ProjectItem>
      <ProjectItem ReplaceParameters="true">MyPlugin1.cs</ProjectItem>
      <ProjectItem OpenInEditor="true">Readme.txt</ProjectItem>
      <ProjectItem>DemoMap.exe</ProjectItem>
    </Project>
  </TemplateContent>
  <PromptForSaveOnCreation>true</PromptForSaveOnCreation>
  <WizardExtension>
